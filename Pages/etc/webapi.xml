<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="GET" url="/V1/pages/howtobuy">
		<service class="MIT\Pages\Api\ContentManagementInterface" method="getContent"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route method="GET" url="/V1/pages/createaccount">
		<service class="MIT\Pages\Api\CreateAccountManagementInterface" method="getCreateAccount"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
</routes>